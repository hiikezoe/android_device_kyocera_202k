on init
    insmod /sbin/mmc_protect.ko

on post-fs
    wait /dev/block/mmcblk0p15
    write /sys/kernel/mmc_protect/clear mmcblk0p15
    wait /dev/block/mmcblk0p16
    write /sys/kernel/mmc_protect/clear mmcblk0p16
    wait /dev/block/mmcblk0p17
    write /sys/kernel/mmc_protect/clear mmcblk0p17

service rmt_storage /system/bin/rmt_storage
    user root
    disabled

on property:ro.emmc=1
    start rmt_storage
